START RESOURCE ..\data\vpnpolins_reg.rss
TARGETPATH  \private\10003a3f\apps
END

LIBRARY         vpnapi.lib
LIBRARY         euser.lib
LIBRARY         apparc.lib 
LIBRARY         efsrv.lib
